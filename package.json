{
	"name": "chessday",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && tsc src/server.ts --outDir build --esModuleInterop --moduleResolution node --module es2022",
		"build:server": "tsc src/server.ts --outDir build --esModuleInterop --moduleResolution node --module es2022",
		"preview": "vite preview",
		"start": "node build/server.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^2.0.0",
		"@sveltejs/kit": "^2.0.0",
		"bcrypt": "^5.1.1",
		"chess.js": "^1.0.0-beta.6",
		"cookie-parser": "^1.4.7",
		"express": "^4.18.2",
		"express-rate-limit": "^7.1.5",
		"helmet": "^7.1.0",
		"ioredis": "^5.3.2",
		"jsonwebtoken": "^9.0.2",
		"mkcert": "^3.2.0",
		"qrcode": "^1.5.3",
		"selfsigned": "^2.4.1",
		"socket.io": "^4.7.2",
		"socket.io-client": "^4.7.2"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
		"@types/bcrypt": "^5.0.2",
		"@types/connect-redis": "^0.0.23",
		"@types/cookie-parser": "^1.4.8",
		"@types/express": "^4.17.21",
		"@types/express-rate-limit": "^5.1.3",
		"@types/express-session": "^1.18.1",
		"@types/helmet": "^0.0.48",
		"@types/http-proxy-middleware": "^0.19.3",
		"@types/ioredis": "^4.28.10",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^20.10.0",
		"@types/qrcode": "^1.5.5",
		"@types/uuid": "^10.0.0",
		"svelte": "^5.0.0-next.1",
		"svelte-check": "^3.6.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	}
}
